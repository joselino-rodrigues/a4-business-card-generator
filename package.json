{
  "name": "a4-business-card-generator",
  "version": "1.0.0",
  "description": "Um pacote npm que permite criar cartões de visita (85x55 mm) e organizá-los automaticamente em uma página A4 (2x5 cartões por página, com margens de 10 mm). O pacote gera um arquivo PDF pronto para impressão.",
  "main": "src/index.js",
  "bin": {
    "a4-business-card": "./bin/cli.js"
  },
  "scripts": {
    "start": "node src/index.js",
    "test": "echo \"Error: no test specified\" && exit 1",
    "dev": "node --watch src/index.js"
  },
  "keywords": [
    "business-card",
    "pdf",
    "a4",
    "generator",
    "print",
    "cartão-visita",
    "pdfkit"
  ],
  "author": "Joselino Rodrigues <joselino.rodrigues@unifesp.br>",
  "license": "MIT",
  "dependencies": {
    "commander": "^11.1.0",
    "pdfkit": "^0.14.0",
    "qrcode": "^1.5.4",
    "sharp": "^0.33.5"
  },
  "devDependencies": {
    "nodemon": "^3.0.2"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/joselino-rodrigues/a4-business-card-generator.git"
  },
  "bugs": {
    "url": "https://github.com/joselino-rodrigues/a4-business-card-generator/issues"
  },
  "homepage": "https://github.com/joselino-rodrigues/a4-business-card-generator#readme"
}
